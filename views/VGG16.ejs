<% include header %>

  <body>

    <nav class="white" role="navigation">
      <div class="nav-wrapper container">
        <ul class="left hide-on-med-and-down">
          <li>
            <a href="/" style="color:black">Back</a>
          </li>
        </ul>
      </div>
    </nav>

    <div class="container">
      <h2>VGG16 Model</h2>
      <%= typeof msg != 'undefined' ? msg : '' %>
        <form action="/VGG16/upload" method="POST" enctype="multipart/form-data">
          <div class="file-field input-field">
            <div class="btn blue">
              <span>File</span>
              <input name="myImage" type="file">
            </div>
            <div class="file-path-wrapper">
              <input class="file-path validate" type="text">
            </div>
          </div>
          <!-- <button type="submit" class="btn">Submit</button> -->
          <button class="btn waves-effect waves-light" type="submit" name="action">Upload
            <i class="material-icons right">send</i>
          </button>
        </form>
        </br>
        <h6 style="color:blue">Image Download URL: </h6>
        <form action="/VGG16/download" method="GET">
          <div class="form-group">
            <input value="https://i.ytimg.com/vi/tjr_SEw76zM/maxresdefault.jpg" name="urlname" id="urlname" type="text" class="validate">
            <label class="active" for="urlname">Download URL</label>
          </div>
          </br>
          <button class="btn waves-effect waves-light" type="submit" name="action">Download
            <i class="material-icons right">send</i>
          </button>
        </form>
        </br>
        <img src="<%= typeof file != 'undefined' ? file : '' %>" width="500" class="responsive-img">
        <form action="/VGG16/predict" method="POST" enctype="multipart/form-data">
          <!-- <button type="submit" class="btn">Submit</button> -->
          <button class="btn waves-effect waves-light" type="submit" name="action">Predict
            <i class="material-icons right">send</i>
          </button>
        </form>
        </br>



        <%if (typeof str != 'undefined') {%>
          <table>
            <tr>
              <th>Label</th>
              <th>Probability Percent</th>
            </tr>
            <% for(var i=0; i < str.length; i++) { %>
              <tr>
                <td>
                  <%= str[i].label %>
                </td>
                <td>
                  <%= str[i].probability*100 %>
                </td>
              </tr>
              <%}%>
                <%}%>
          </table>
    </div>


    <% include footer %>